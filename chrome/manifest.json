{
  "manifest_version": 2,
  "name": "TickPlate",
  "short_name": "TickPlate",
  "version": "1.0.0",
  "description": "Todo list for creatives",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_title": "TickPlate",
    "default_popup": "index.html"
  },
  "permissions": ["storage"]
}
